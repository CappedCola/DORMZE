{
  "expo": {
    "name": "Dormze",
    "slug": "dormze",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/DormzeApplicationIcon.png",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "io.dormze.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Allow Dormze to access your photos to add profile pictures and share images with roommates.",
        "NSCameraUsageDescription": "Allow Dormze to access your camera to take profile pictures.",
        "NSMicrophoneUsageDescription": "Allow Dormze to access your microphone for videos with audio."
      },
      "buildNumber": "5"
    },
    "android": {
      "package": "io.dormze.app",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "description": "A social app for college students to connect in their dorms.",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b17bea3a-4d49-44f7-b022-e293b7b362c4"
      }
    },
    "owner": "cappedcola",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/b17bea3a-4d49-44f7-b022-e293b7b362c4"
    }
  }
}
